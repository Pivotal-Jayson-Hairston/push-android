<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pivotal.cf.mobile.analyticssdk.sample">

    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.pivotal.cf.mobile.analyticssdk.sample.activity.MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.pivotal.cf.mobile.analyticssdk.sample.activity.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.pivotal.cf.mobile.analyticssdk.sample.activity.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pivotal.cf.mobile.analyticssdk.sample.activity.MainActivity" />
        </activity>

    </application>

</manifest>
